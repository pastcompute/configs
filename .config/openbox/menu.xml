<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3">
		<item label="gedit (sudo)">
			<action name="Execute">
				<execute>sudo gedit</execute>
			</action>
		</item>
		<item label="Synaptic (sudo)">
			<action name="Execute">
				<execute>sudo synaptic</execute>
			</action>
		</item>
		<separator/>
		<item label="Eterm">
			<action name="Execute">
				<execute>Eterm</execute>
			</action>
		</item>
		<item label="Xterm">
			<action name="Execute">
				<execute>xterm -sl 10000 -sb</execute>
			</action>
		</item>
		<item label="Terminal emulator">
			<action name="Execute">
				<execute>x-terminal-emulator</execute>
			</action>
		</item>
		<separator/>
		<item label="gedit">
			<action name="Execute">
				<execute>gedit</execute>
			</action>
		</item>
		<item label="RedNotebook">
			<action name="Execute">
				<execute>rednotebook</execute>
			</action>
		</item>
		<separator/>
		<item label="Kaffeine">
			<action name="Execute">
				<execute>kaffeine</execute>
			</action>
		</item>
		<item label="Amarok">
			<action name="Execute">
				<execute>amarok</execute>
			</action>
		</item>
		<separator/>
		<item label="Web browser">
			<action name="Execute">
				<execute>x-www-browser</execute>
			</action>
		</item>
		<item label="KeePass2">
			<action name="Execute">
				<execute>keepass2</execute>
			</action>
		</item>
		<separator/>
		<!-- This requires the presence of the 'menu' package to work -->
		<menu id="/Debian"/>
		<separator/>
		<menu id="client-list-menu"/>
		<separator/>
		<item label="ObConf">
			<action name="Execute">
				<execute>obconf</execute>
			</action>
		</item>
		<item label="Reconfigure">
			<action name="Reconfigure"/>
		</item>
		<item label="Restart">
			<action name="Restart"/>
		</item>
		<separator/>
		<item label="Exit">
			<action name="Exit"/>
		</item>
	</menu>
</openbox_menu>
